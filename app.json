{
  "expo": {
    "name": "gimme-x",
    "slug": "gimme-x",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.gimme",
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app uses Face ID to authenticate you."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#374BFB"
      },
      "package": "com.anonymous.gimme"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "backgroundColor": "#374BFB",
          "imageWidth": 200,
          "dark": {
            "image": "./assets/images/splash.png",
            "backgroundColor": "#000000"
          },
          "resizeMode": "contain"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bb443fd3-6af0-487c-bef4-64538e5be33c"
      }
    }
  }
}
